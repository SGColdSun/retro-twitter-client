{
  "name": "retro-twitter-client",
  "private": true,
  "version": "0.0.2",
  "main": "app/browser/index.js",
  "scripts": {
    "clean": "rm -rf packages/v0.0.2/{retro-twitter-client-{darwin,win32}-x64}",
    "compile": "gulp compile",
    "package": "electron-packager . retro-twitter-client --arch=x64 --out=packages/v0.0.2 --platform=darwin,win32 --version=0.31.0",
    "release": "npm run zip && npm run clean && npm run upload",
    "setup": "npm install && brew tap tcnksm/ghr && brew install ghr",
    "start": "electron .",
    "upload": "ghr v0.0.2 packages/",
    "watch": "gulp watch",
    "zip": "npm run zip-darwin && npm run zip-win32",
    "zip-darwin": "cd packages/v0.0.2 && zip -r retro-twitter-client-darwin-x64.zip retro-twitter-client-darwin-x64",
    "zip-win32": "cd packages/v0.0.2 && zip -r retro-twitter-client-win32-x64.zip retro-twitter-client-win32-x64"
  },
  "repository": "r7kamura/retro-twitter-client",
  "author": "Ryo Nakamura <r7kamura@gmail.com> (https://github.com/r7kamura)",
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^5.0.2",
    "electron-prebuilt": "^0.31.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-sass": "^2.0.4",
    "gulp-watch": "^4.3.5"
  },
  "dependencies": {
    "HTML5-Reset": "git://github.com/r7kamura/HTML5-Reset.git",
    "font-awesome": "^4.4.0",
    "node-twitter-api": "^1.6.0",
    "qiita-coat": "git://github.com/increments/qiita-coat.git#d751df7df60d0dcd0c97b9dc05c8ab5da3db22e3",
    "react": "^0.13.3",
    "twitter": "^1.2.5"
  }
}
